class Solution {
    public int firstMissingPositive(int[] nums) {
        Arrays.sort(nums);
        int min = 1;
        for(int i =0;i<nums.length;i++){
            if(nums[i] == min)
                min+=1;
            else if(nums[i]>min)
                return min;
        }
        return min;
    }
        
}
